{
  "serverName": "deno-mcp-demo",
  "version": "0.1.0",
  "httpPort": 8787,
  "features": {
    "enableHttpAdmin": true,
    "enableCodeEditing": true,
    "enableMcpSse": true,
    "enableMcpHttp": true
  },
  "cors": {
    "allowedOrigins": [
      "https://*.heatonz.deno.net",
      "http://localhost:*",
      "https://localhost:*",
      "http://127.0.0.1:*",
      "https://127.0.0.1:*"
    ]
  },
  "upstreams": [
    {
      "name": "context7-proxy",
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "namespace": "context7",
      "enabled": true
    }
  ]
} 